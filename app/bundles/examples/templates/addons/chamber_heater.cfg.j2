# chamber_heater add-on
# Generated by KlippConfig bundle template

[heater_generic chamber]
heater_pin: {{ override("pins.chamber_heater", "PA0") }}
sensor_type: Generic 3950
sensor_pin: {{ override("pins.chamber_thermistor", "PC3") }}
control: pid
pid_Kp: {{ override("chamber.pid_kp", 36.0) }}
pid_Ki: {{ override("chamber.pid_ki", 1.2) }}
pid_Kd: {{ override("chamber.pid_kd", 280.0) }}
max_power: {{ override("chamber.max_power", 0.6) }}
min_temp: {{ override("chamber.min_temp", 0) }}
max_temp: {{ override("chamber.max_temp", 80) }}

[temperature_fan chamber]
pin: {{ override("pins.chamber_fan", "PA1") }}
sensor_type: Generic 3950
sensor_pin: {{ override("pins.chamber_thermistor", "PC3") }}
target_temp: {{ override("chamber.target_temp", 45) }}
max_speed: {{ override("chamber.fan_max_speed", 1.0) }}
