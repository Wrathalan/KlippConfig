# Generated by KlippConfig
# Preset: {{ preset.name }} ({{ preset.id }})
# Board: {{ project.board }}

[include mcu.cfg]
[include board_pins.cfg]
{% if project.toolhead.enabled %}
[include toolhead.cfg]
[include toolhead_pins.cfg]
{% endif %}
[include motion.cfg]
[include thermal.cfg]
[include input_shaper.cfg]
{% if project.leds.enabled %}
[include leds.cfg]
{% endif %}
{% if project.addons %}
[include addons.cfg]
{% endif %}
{% if project.macro_packs and preset.feature_flags.macros_supported %}
[include macros.cfg]
{% endif %}

[printer]
kinematics: {{ preset.kinematics }}
max_velocity: {{ motion.max_velocity }}
max_accel: {{ motion.max_accel }}
square_corner_velocity: {{ motion.square_corner_velocity }}

[idle_timeout]
timeout: {{ override("printer.idle_timeout", 1800) }}


